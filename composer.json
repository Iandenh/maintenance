{
    "name": "php-middleware/maintenance",
    "description": "Site maintenance middleware SEO friendly with PSR-7",
    "type": "library",
    "license": "MIT",
    "keywords": [
        "middleware",
        "psr",
        "psr-7",
        "maintenance"
    ],
    "require": {
        "php": ">=5.6",
        "psr/http-message": "^1.0",
        "php-middleware/double-pass-compatibility": "^1.0",
        "http-interop/http-middleware": "^0.4.1"
    },
    "require-dev": {
        "phpunit/phpunit": "^5.6 || ^6.1",
        "zendframework/zend-diactoros": "^1.1.3"
    },
    "autoload": {
        "psr-4": {
            "PhpMiddleware\\Maintenance\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "PhpMiddlewareTest\\Maintenance\\": "test/"
        }
    }
}
